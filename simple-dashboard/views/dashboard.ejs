  <script>
    $(document).ready(function() {
      // Javascript method's body can be found in assets/js/demos.js
      // md.initDashboardPageCharts();

      // Change active state of sidebar buttons.
      $('.sidebar-nav a[data-toggle="tab"]').on('show.bs.tab', function (e) {
        $('.sidebar-nav .nav-item.active').removeClass('active')
        $(e.target).parent(".nav-item").addClass("active") // newly activated tab
        $(e.relatedTarget).parent(".nav-item").removeClass("active") // previous active tab
      })

    });
  </script>
